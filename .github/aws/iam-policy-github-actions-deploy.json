{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBucketsForDeploy",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::rocketseat-upload-widget-web-statics",
        "arn:aws:s3:::rocketseat-upload-widget-web-cdn"
      ]
    },
    {
      "Sid": "ObjectAccessForDeploy",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:AbortMultipartUpload"
      ],
      "Resource": [
        "arn:aws:s3:::rocketseat-upload-widget-web-statics/*",
        "arn:aws:s3:::rocketseat-upload-widget-web-cdn/*"
      ]
    }
  ]
}