{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowGithubActionsDeploy",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::123456789012:user/github-actions-deploy"
      },
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:AbortMultipartUpload"
      ],
      "Resource": "arn:aws:s3:::rocketseat-upload-widget-web-cdn/*"
    },
    {
      "Sid": "AllowGithubActionsListBucket",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::123456789012:user/github-actions-deploy"
      },
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::rocketseat-upload-widget-web-cdn"
    }
  ]
}