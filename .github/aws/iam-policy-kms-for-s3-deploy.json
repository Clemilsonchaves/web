{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowUseOfKmsKeyForS3Uploads",
      "Effect": "Allow",
      "Action": [
        "kms:Encrypt",
        "kms:Decrypt",
        "kms:GenerateDataKey",
        "kms:DescribeKey"
      ],
      "Resource": "arn:aws:kms:us-east-1:123456789012:key/00000000-0000-0000-0000-000000000000"
    }
  ]
}